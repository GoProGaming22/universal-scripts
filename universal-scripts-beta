-- Funkcja do przełączania zakładek
local function showTab(tabName)
    ScrollingFrameHome.Visible = false
    ScrollingFramePremium.Visible = false

    if tabName == "Home" then
        ScrollingFrameHome.Visible = true
    elseif tabName == "Premium" then
        ScrollingFramePremium.Visible = true
    end
    print("[Tab Switch]: Przełączono na zakładkę: " .. tabName) -- Dodano do output
end

-- Inicjalne ustawienie: pokaż Home tab
showTab("Home")

-- Sprawdzenie Game Passa
local hasGamePass = false
local success, result = pcall(function()
    hasGamePass = MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
end)

if success then
    PremiumTabButton.Visible = hasGamePass
    if not hasGamePass then
        sendNotification("Universal Scripts", "Brak Game Passa Premium. Zakładka Premium ukryta.")
    else
        sendNotification("Universal Scripts", "Posiadasz Game Pass Premium! Zakładka Premium dostępna.")
    end
else
    warn("Błąd podczas sprawdzania Game Passa:", result)
    PremiumTabButton.Visible = false -- Ukryj na wszelki wypadek
    sendNotification("Universal Scripts", "Nie można sprawdzić Game Passa. Zakładka Premium ukryta.")
end

-- Ładowanie skryptów (zmienione referencje przycisków na nowe nazwy)

-- Przyciski Home
InvisibiltyButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()
