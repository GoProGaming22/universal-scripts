-- Obsługa przycisków zakładek
HomeTabButton.MouseButton1Click:Connect(function()
    showTab("Home")
end)

PremiumTabButton.MouseButton1Click:Connect(function()
    showTab("Premium")
end)

-- Obsługa przycisku ukrywania/pokazywania GUI
HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
    print("[Hide/Show]: GUI Visible: " .. tostring(MainFrame.Visible))
end)

-- Powitalne powiadomienie po krótkiej chwili
spawn(function()
    wait(2)
    sendNotification("Universal Scripts", "Dzięki za używanie Universal Scripts! Witaj w nowej wersji!")
end)

-- Czekanie na załadowanie GUI
task.wait(0.5)
print("Universal Scripts: GUI załadowane i gotowe.")
