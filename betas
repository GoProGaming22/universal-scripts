-- Properties (POCHODZĄCE Z TWOJEGO KODU - Zmodyfikowano tylko parent i pozycję HideButton)

ScreenGui.Name = "UniversalScriptsGui"
ScreenGui.Parent = player:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Frame.Name = "MainFrame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0.172549, 0.172549, 0.172549)
Frame.BorderColor3 = Color3.new(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.212787732, 0, 0.244107679, 0)
Frame.Size = UDim2.new(0.513162374, 0, 0.560262501, 0)
Frame.Draggable = true
Frame.Visible = true

UICorner.Parent = Frame

-- Zmodyfikowany HideButton - Parent na ScreenGui, NIE Draggable, ImageButton
HideButton.Name = "HideButton"
HideButton.Parent = ScreenGui -- Parent to ScreenGui
HideButton.BackgroundColor3 = Color3.new(1, 0, 0) -- Nadal możesz mieć kolor tła
HideButton.BackgroundTransparency = 1 -- Zwykle dla ImageButton tło jest przezroczyste
HideButton.BorderColor3 = Color3.new(0, 0, 0)
HideButton.BorderSizePixel = 3
HideButton.Position = UDim2.new(0.5, 0, 0.5, 0) -- Pozycja początkowa, będzie ustawiona dynamicznie raz
HideButton.Size = UDim2.new(0.0482946634, 0, 0.0802469105, 0) -- Utrzymujemy Twój rozmiar
HideButton.ZIndex = 3 -- Wyższa wartość ZIndex, aby był zawsze na wierzchu
HideButton.Image = "rbxassetid://109075000814284" -- ZAKTUALIZOWANE ID OBRAZKA!
HideButton.Draggable = false -- ZMIANA TUTAJ: Wyłączamy przeciąganie dla X, aby był niezależny
HideButton.ScaleType = Enum.ScaleType.Fit -- Upewnij się, że obrazek dobrze się skaluje

UITextSizeConstraint.Parent = HideButton
UITextSizeConstraint.MaxTextSize = 28
