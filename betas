-- PoÅ‚Ä…czenia przyciskÃ³w Premium (z ponownym sprawdzeniem Game Passa)
SpectateButtonPremium.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success and currentHasGamePass then
        loadstring(game:HttpGet("https://pastebin.com/raw/UFVLWQRw"))()
        sendNotification("NarzÄ™dzie Obserwacji", "Skrypt obserwacji zaÅ‚adowany!")
    else
        sendNotification("Universal Scripts", "Musisz posiadaÄ‡ Game Pass Premium, aby uÅ¼yÄ‡ tej funkcji!")
        if success then -- Tylko jeÅ›li pcall siÄ™ powiÃ³dÅ‚, ale gracz nie ma Game Passa
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
        end
    end
end)

