-- Tworzenie ImageButton (przycisk "X")
local HideButton = Instance.new("ImageButton")
HideButton.Name = "HideButton"
HideButton.Parent = ScreenGui -- Ważne: jest dzieckiem ScreenGui, nie MainFrame
HideButton.BackgroundColor3 = Color3.new(1, 1, 1)
HideButton.BorderColor3 = Color3.new(0, 0, 0)
HideButton.BorderSizePixel = 0
HideButton.Position = UDim2.new(0.707863092, 0, 0.214006022, 0) -- Początkowa pozycja
HideButton.Size = UDim2.new(0, 52, 0, 52)
HideButton.Image = "rbxassetid://102860171689689" -- Upewnij się, że to ID obrazu jest poprawne
HideButton.ZIndex = 2 -- Upewnij się, że jest nad innymi elementami

--- Logika działania GUI ---

-- Funkcja do przełączania widoczności MainFrame
HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible -- Przełącza widoczność głównego okna GUI
end)

-- Logika przesuwania ImageButton (przycisk "X")
local isDragging = false
local dragStartPos = Vector2.new(0, 0)
local buttonStartPos = UDim2.new(0, 0, 0, 0)

HideButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        isDragging = true
        dragStartPos = input.Position
        buttonStartPos = HideButton.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if isDragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStartPos
        HideButton.Position = UDim2.new(0, buttonStartPos.X.Offset + delta.X, 0, buttonStartPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        isDragging = false
    end
end)

-- Logika przełączania zakładek
HomeButton.MouseButton1Click:Connect(function()
    ScrollingFrameHome.Visible = true
    ScrollingFramePremium.Visible = false
end)

PremiumButton.MouseButton1Click:Connect(function()
    ScrollingFrameHome.Visible = false
    ScrollingFramePremium.Visible = true
end)

-- Ustawienie domyślnej zakładki przy starcie skryptu
ScrollingFrameHome.Visible = true
ScrollingFramePremium.Visible = false
