-- Logika przyciskÃ³w przeÅ‚Ä…czajÄ…cych zakÅ‚adki
HomeTabButton.MouseButton1Click:Connect(function()
    showTab("Home")
end)

PremiumTabButton.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success then
        if currentHasGamePass then
            showTab("Premium")
            sendNotification("Universal Scripts", "PrzeÅ‚Ä…czono na zakÅ‚adkÄ™ Premium.")
        else
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
            sendNotification("Universal Scripts", "Game Pass Premium nie znaleziony. Otwieram okno zakupu...")
        end
    else
        warn("BÅ‚Ä…d podczas sprawdzania Game Passa po klikniÄ™ciu Premium:", currentHasGamePass)
        sendNotification("Universal Scripts", "WystÄ…piÅ‚ bÅ‚Ä…d podczas sprawdzania Game Passa.")
    end
end)


HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- ObsÅ‚uga zdarzenia po zakoÅ„czeniu prÃ³by zakupu Game Passa
MarketplaceService.PromptGamePassPurchaseFinished:Connect(function(userId, gamePassId, wasPurchased)
    if userId == player.UserId and gamePassId == PREMIUM_GAMEPASS_ID and wasPurchased then
        hasGamePassAtStartup = true -- Aktualizujemy tylko zmiennÄ… do celÃ³w informacyjnych
        sendNotification("Universal Scripts", "Game Pass Premium kupiony pomyÅ›lnie! Ciesz siÄ™ nowymi funkcjami.")
        showTab("Premium") -- Automatycznie przeÅ‚Ä…cz na zakÅ‚adkÄ™ Premium po zakupie
    elseif userId == player.UserId and gamePassId == PREMIUM_GAMEPASS_ID and not wasPurchased then
        sendNotification("Universal Scripts", "Anulowano zakup Game Passa Premium.")
    end
end)

-- PoczÄ…tkowe powiadomienie
spawn(function()
    wait(2)
    sendNotification("Universal Scripts", "DziÄ™ki za uÅ¼ywanie Universal Scripts! Witaj w nowej wersji!")
end)

task.wait(0.5)
