-- Instances (Instancje - Tworzenie elementów GUI)

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
local TextBox = Instance.new("TextBox")
local ScrollingFrame = Instance.new("ScrollingFrame")
local Invisibiltybutton = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local SpeedToolbutton = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local ClicktptoolButton = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")
local HideButton = Instance.new("TextButton") -- Przycisk do ukrywania/pokazywania panelu

-- Properties (Właściwości - Ustawianie wyglądu i pozycji)

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui") -- Ważne: Umieszcza GUI w PlayerGui gracza
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0.172549, 0.172549, 0.172549)
